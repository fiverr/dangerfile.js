{
  "extends": [
    "@fiverr/eslint-config-fiverr/rules/base",
    "@fiverr/eslint-config-fiverr/rules/es6",
    "@fiverr/eslint-config-fiverr/rules/jest"
  ],
  "overrides": [
    {
      "files": [ "bin.js" ],
      "rules": {
        "no-console": 0
      }
    },
    {
      "files": [ "dangerfile.js" ],
      "globals": {
        "danger": true,
        "fail": true,
        "warn": true,
        "message": true
      }
    }
  ]
}
